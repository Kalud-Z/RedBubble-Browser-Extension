{
  "manifest_version": 2,

  "name": "RedBubble",
  "description": "A very helpful side-quick for sellers in RedBubble",
  "version": "1.0.0",
  "icons": { "128": "icon_128.png" },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts" : [
    {
      "matches" : [ "https://www.redbubble.com/portfolio/images/*" ],
      "js" : [ "content.js" ]
    }
  ]
}

